!URxvt.background: black
!URxvt.foreground: green
!URxvt.cursorColor: yellow
!URxvt.underlineColor: green
!URxvt.font:xft:Bitstream Vera Sans Mono:pixelsize=12:antialias=true
URxvt.font:xft:Hack:pixelsize=16:antialias=true
xftp.dpi: 120
!------------
!True Transparency
!------------
!set to 32-bit for real transparency (compositing required (see xcompmgr)
urxvt*depth:             32
!transparent=0000 opaque=ffff
!urxvt*background: rgba:1111/1111/1111/bbbb
!urxvt*background: rgba:1111/1111/1111/eeee
urxvt*background: rgba:1111/1111/1111/ffff
!urxvt*background: rgba:1111/1111/1111/cccc
!urxvt*background: rgba:1111/1111/1111/0000


!------------------
!False tranparency
!------------------
!URxvt.transparent: true
!URxvt.shading: 50 

!Urxvt.secondaryScroll:  true    # Enable Shift-PageUp/Down in screen
URxvt.scrollBar: false

Xcursor.size: 16
URxvt*borderLess: false
